{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/maker.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/created.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/maker",
      "methods": ["POST"],
      "dest": "/api/maker.js"
    },
    {
      "src": "/created/(.*)",
      "methods": ["POST"],
      "dest": "/api/created.js"
    },
    {
      "src": "/verify/(.*)",
      "methods": ["GET"],
      "dest": "/api/created.js"
    },
    {
      "src": "/resolve-session",
      "methods": ["POST"],
      "dest": "/api/created.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
