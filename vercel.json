{
  "version": 2,
  "builds": [
    {
      "src": "maker.js",
      "use": "@vercel/node"
    },
    {
      "src": "created.js",
      "use": "@vercel/node"
    },
    {
      "src": "getToken.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/created(.*)",
      "dest": "created.js"
    },
    {
      "src": "/api/getToken",
      "dest": "getToken.js"
    },
    {
      "src": "/(.*)",
      "dest": "maker.js"
    }
  ]
}
